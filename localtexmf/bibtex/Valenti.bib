@inproceedings{seguin:96,
address={Canada},
annote={We show that there is a MacWilliams type identity relating the input-redundancy weight enumerator A(W,Z) (this is a weight-enumerator which is used to bound the bit-error-rate of the code) of a turbo block code and the input-redundancy weight enumerator B(W,Z) of the dual code. Following Benedetto and Montorsi we obtain an explicit formula for the average B(W,Z) averaged over all permutations. We explicitly compute B(W,Z) for a turbo code based on Hamming's single- error correcting code and compare it to A(W,Z) as obtained by Benedetto and Montorsi. We further obtain expressions for A(W,Z) for a turbo code based on single parity bit constituent codes, and where the permutation is a cyclic shift (1 Refs.)},
author={G. E. Seguin and S. Fournier},
booktitle={Proc., 18th Biennial Symp. on Commun.},
editor={H.T. Mouftah},
pages={139-140},
publisher={Queen's Univ; Kingston, Ont., Canada},
title={The dual of a turbo block code},
year={1996}
}

