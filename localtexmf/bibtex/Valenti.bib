@inproceedings{seghers:95,
annote={The performance of TURBO codes is addressed by examining the distance spectrum. The error floor' is shown to be a consequence of the relatively low free distance of the code. It is also shown that the error floor' can be lowered by increasing the size of the interleaver. The excellent performance of TURBO codes at low signal-to-noise ratios is explained in terms of the distance spectrum. The interleaver in the TURBO encoder is shown to reduce the number of low weight codewords through a process called spectral thinning'. This thinned distance spectrum results in the free distance asymptote being the dominant performance parameter for low and moderate signal-to-noise ratios. Finally, it is shown that for a fixed interleaver size, the performance of TURBO codes may be improved by using a primitive polynomial as the feedback polynomial in the encoder (7 Refs.)},
author={J. Seghers and L. C. Perez and D. J. Costello},
booktitle={Proc., Allerton Conf. on Commun., Control, and Computing},
pages={357-365},
publisher={Univ. Illinois; Urbana-Champaign, IL, USA},
title={On selecting code generators for turbo codes},
year={1995}
}

