@inproceedings{fazel:95,
annote={Different concepts of iterative decoding algorithms for serial or parallel concatenated coding (called turbo-code) as well as for the special case of the generalized concatenated coding schemes have been introduced. These algorithms being very efficient, are based on soft output decoding, where, the soft decisions are used as feed-forward information for the next steps of iterations. In order to achieve the high performance expected by turbo-codes and to assume a great flexibility for the system, we combine a multilevel turbo-code with 8PSK modulation. This allow us to examine in detail the performance of this combination compared with different coding and decoding approaches: turbo-code with Gray mapping and iterative multistage decoding using convolutional codes or a combination of turbo codes, convolutional and single parity check codes. In all of our studies we consider a super-concatenated coding scheme, where the outer code is based on Reed-Solomon code. The results showed that the coding gain at a BER of 10/sup - 4/ (since we employ a powerful outer RS decoder) for the iterative convolutional coding and for the combination of convolutional/turbo coding is quite the same compared to turbo coding with Gray mapping, which is 0.5 dB. However, compared to uncoded QPSK, the coding gain is 4 dB (at a BER of 10/sup -4/), which is quite significant (11 Refs.)},
author={K. Fazel and L. Papke},
booktitle={Proc., IEEE GLOBECOM},
pages={649-653},
title={Combined multilevel turbo-code with {8PSK} modulation},
year={1995}
}

@inproceedings{fazel:96,
annote={The aim of this study is to look at the performance of an iterative decoding algorithm applied to the decoding of Blokh-Zyablov (1974) codes using non-systematic and non- recursive convolutional codes; where, the decoding principle can be also applied to block codes. In addition, this study allows us to compare this coding scheme with the turbo code and with a single convolutional code. The results showed that the coding gain at a BER of 10/sup -4/ for the iterative BZ-code and for turbo coding is quite the same compared to convolutional coding in AWGN and in Rayleigh- fading channels. This gain for both coding schemes (BZ and turbo) using a soft-output Viterbi-decoding algorithm in AWGN and in Rayleigh-fading channels is 1.3 dB and 1.5 dB, respectively. Compared to uncoded BPSK, the coding gain for both schemes is much higher: 6.3 dB in AWGN and about 30 dB in a Rayleigh fading channel; which is quite significant (10 Refs.)},
author={K. Fazel},
booktitle={Proc., IEEE Int. Conf. on Commun.},
